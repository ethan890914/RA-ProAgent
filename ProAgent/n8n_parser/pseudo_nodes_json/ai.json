{
    "displayName": "Ai Completion",
    "name": "n8n-nodes-base.aiCompletion",
    "group": [
        "transform"
    ],
    "version": 1,
    "description": "Use GPT from openAi to complete language tasks.",
    "defaults": {
        "name": "Ai Completion"
    },
    "inputs": [
        "main"
    ],
    "outputs": [
        "main"
    ],
    "credentials": [
        {
            "name": "actionNetworkApi",
            "required": false
        }
    ],
    "properties": [
        {
            "displayName": "Resource",
            "name": "resource",
            "type": "options",
            "noDataExpression": true,
            "options": [
                {
                    "name": "default",
                    "value": "default"
                }
            ],
            "default": "default"
        },
        {
            "displayName": "Operation",
            "name": "operation",
            "type": "options",
            "noDataExpression": true,
            "displayOptions": {
                "show": {
                    "resource": [
                        "default"
                    ]
                }
            },
            "options": [
                {
                    "name": "default",
                    "value": "default",
                    "action": "chat with AI"
                }
            ],
            "default": "default"
        }
    ],
    "properties_note": "⚠️ PSEUDO NODE: aiCompletion does NOT use params! It reads messages from input_data only. Build input_data in mainWorkflow as: [{\"json\": {\"messages\": [{\"role\": \"system\", \"content\": \"...\"}, {\"role\": \"user\", \"content\": \"...\"}]}}]",
    "pseudoNode": true
}
